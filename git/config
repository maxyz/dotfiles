[core]
	editor = vim
[init]
	defaultBranch = main
[merge]
	tool = vimdiff
[pull]
	ff = only
[push]
	default = upstream
[rebase]
	stat = true
[rerere]
	enabled = true
	autoupdate = true
[tar "tar.xz"]
	command = xz -c

[alias]
	co = checkout
	br = branch
	ci = commit
	st = status

[color]
	branch = auto
	diff = auto
	grep = auto
	interactive = auto
	status = auto
	ui = auto

[url "https://github.com/"]
	insteadof = github:
[url "git@github.com:"]
	pushInsteadof = github:
[url "git@github.com:"]
	pushInsteadof = https://github.com/
